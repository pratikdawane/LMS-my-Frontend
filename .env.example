# ============================================
# FRONTEND ENVIRONMENT CONFIGURATION
# ============================================
# For local development, copy this to .env
# For production (Render), set VITE_API_BASE_URL in Render dashboard
# ============================================

# Backend API Base URL
# Local: http://localhost:4000/api
# Production: https://lms-backend-o6am.onrender.com/api
VITE_API_BASE_URL=http://localhost:4000/api

# Frontend Port (optional, defaults to 5173)
VITE_PORT=5173

# API Request Timeout in milliseconds (optional, defaults to 30000)
VITE_API_TIMEOUT=30000
